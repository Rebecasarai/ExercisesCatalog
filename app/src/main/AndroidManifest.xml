<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="rebecasarai.github.com.exercisescatalog">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"><intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>
        <activity
            android:name=".FontF"
            android:label="Font Fun">

        </activity>
        <activity
            android:name=".Calculator"
            android:label="Calculadora"></activity>
        <activity android:name=".MemoryGame" />
        <activity android:name=".RadioButtonCalculator" />
        <activity android:name=".ReconocimientoImagen" />
        <activity
            android:name=".Galeria"
            android:label="@string/title_activity_galeria"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="rebecasarai.github.com.exercisescatalog.MainActivity" />
        </activity>
    </application>

</manifest>